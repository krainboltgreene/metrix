{
  "name": "metrix",
  "version": "1.0.0",
  "description": "The lifeblood of your product: Metadata of the data.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm-run-all --parallel build:client build:assets",
    "build:client": "mkdir -p './tmp/' && browserify --transform babelify --transform cssify --entry './client/index.js' --outfile './tmp/index.js'",
    "build:assets": "gulp build",
    "watch": "npm-run-all --parallel watch:client watch:assets",
    "watch:client": "mkdir -p './tmp/' && watchify --transform babelify --transform cssify --entry './client/index.js' --outfile './tmp/index.js'",
    "watch:assets": "gulp watch",
    "lint": "eslint './client/' './remote/'",
    "test": "npm-run-all --parallel test:client test:worker test:remote",
    "test:client": "mocha --compilers js:babel-register './client/**/test.js'",
    "test:worker": "mocha --compilers js:babel-register './worker/**/test.js'",
    "test:remote": "mocha --compilers js:babel-register './remote/**/test.js'",
    "start": "npm-run-all --parallel watch server",
    "server": "npm-run-all --parallel server:postgres server:http server:workers",
    "server:postgres": "postgrest postgres://metrix:@localhost:5432/metrix --port 8081 --anonymous metrix --schema public",
    "server:http": "http-server -c-1 -p 8080 './tmp/' -o",
    "server:workers": "babel-node './worker/index.js'",
    "data:migrate": "knex migrate:latest",
    "data:change": "knex migrate:make",
    "data:seed": "knex seed:run"
  },
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurelandwolf/metrix.git"
  },
  "keywords": [
    "metrics",
    "dashboard",
    "data",
    "analysis"
  ],
  "bugs": {
    "url": "https://github.com/laurelandwolf/metrix/issues"
  },
  "homepage": "https://laurelandwolf.github.io/metrix",
  "engines": {
    "node": "^6",
    "npm": "^3"
  },
  "devDependencies": {
    "babel": "^6",
    "babel-cli": "^6",
    "babel-eslint": "^6",
    "babel-plugin-closure-elimination": "^1",
    "babel-plugin-implicit-return": "^1",
    "babel-plugin-import-asserts": "^2",
    "babel-plugin-ramda": "^1",
    "babel-plugin-relative-import": "^1.0.3",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-es2015-arrow-functions": "^6",
    "babel-plugin-transform-es2015-computed-properties": "^6",
    "babel-plugin-transform-es2015-destructuring": "^6",
    "babel-plugin-transform-es2015-function-name": "^6",
    "babel-plugin-transform-es2015-literals": "^6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6",
    "babel-plugin-transform-es2015-parameters": "^6",
    "babel-plugin-transform-es2015-shorthand-properties": "^6",
    "babel-plugin-transform-es2015-spread": "^6",
    "babel-plugin-transform-es2015-sticky-regex": "^6",
    "babel-plugin-transform-es2015-typeof-symbol": "^6",
    "babel-plugin-transform-es2015-unicode-regex": "^6",
    "babel-plugin-transform-es5-property-mutators": "^6",
    "babel-plugin-transform-export-extensions": "^6",
    "babel-plugin-transform-function-bind": "^6",
    "babel-plugin-transform-member-expression-literals": "^6",
    "babel-plugin-transform-object-assign": "^6",
    "babel-plugin-transform-object-rest-spread": "^6",
    "babel-plugin-transform-object-set-prototype-of-to-assign": "^6",
    "babel-plugin-transform-property-literals": "^6",
    "babel-plugin-transform-proto-to-assign": "^6",
    "babel-preset-react": "^6",
    "babelify": "^7",
    "browserify": "^13",
    "chai": "^3",
    "cssify": "^1.0.3",
    "eslint": "^2",
    "eslint-plugin-babel": "^3",
    "eslint-plugin-immutable": "^1",
    "eslint-plugin-import": "^1",
    "eslint-plugin-mocha": "^4",
    "eslint-plugin-node": "^2",
    "eslint-plugin-react": "^5",
    "gulp": "^3",
    "gulp-cli": "^1",
    "gulp-concat": "^2",
    "mocha": "^2",
    "redux-devtools": "^3",
    "redux-devtools-diff-monitor": "^5",
